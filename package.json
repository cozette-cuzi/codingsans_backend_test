{
  "name": "codingsans-backend-test",
  "version": "1.0.0",
  "repository": "git@gitlab.com:codingsans/public/codingsans-backend-test.git",
  "author": "LaTotty <latotty@codingsnas.com>",
  "license": "MIT",
  "scripts": {
    "start": "ts-node src/index.ts",
    "test": "jest --config package.json",
    "lint": "eslint src --ext .ts"
  },
  "devDependencies": {
    "@codingsans/eslint-config": "4.0.0",
    "@types/bcrypt": "5.0.0",
    "@types/config": "0.0.41",
    "@types/express": "4.17.13",
    "@types/jest": "24.9.0",
    "@types/morgan": "1.9.3",
    "@types/node": "13.1.8",
    "bcrypt": "5.0.1",
    "config": "3.3.7",
    "eslint": "6.8.0",
    "jest": "24.9.0",
    "mongoose": "6.2.4",
    "morgan": "1.10.0",
    "prettier": "1.19.1",
    "ts-jest": "24.3.0",
    "ts-node": "8.6.2",
    "typescript": "3.7.5"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.d.ts",
      "!src/**/*.factory.ts",
      "!src/commands/*"
    ],
    "coverageReporters": [
      "html",
      "json"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "dependencies": {
    "dotenv": "16.0.0",
    "express": "4.17.3"
  }
}
